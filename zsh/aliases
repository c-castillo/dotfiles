alias lg='lazygit'

alias -g ...='../..'
alias -g ....='../../..'
alias -g .....='../../../..'
alias -g ......='../../../../..'
alias -- -='cd -'
alias md='mkdir -p'
alias d='dirs -v | head -10'
alias lsa='ls -lah'
alias l='ls -lah'
alias ll='ls -lh'
alias la='ls -lAh'
alias cat='bat'
ls -G . &>/dev/null && alias ls='ls -G'
